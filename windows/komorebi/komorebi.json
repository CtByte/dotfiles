  //  "palette": "Base16",
  //  "name": "CatppuccinLatte",
  //  "bar_accent": "Base0D",
  //  "unfocused_border": "Base03"
  //},
  "display_index_preferences": {
    "0": "JZWDBH3", // "DEL4279-5&1047835a&1&UID4354",
    "1": "9H579H3", // "DEL4277-5&1047835a&1&UID4352",
    "2": "0" // "AUOC199-4&3138360a&0&UID8388688"
  },
  "bar_configurations": [
    "$Env:USERPROFILE/komorebi.bar.json",
    "$Env:USERPROFILE/komorebi.bar.2.json",
    "$Env:USERPROFILE/komorebi.bar.3.json"
  ],
  "monitors": [
    {
      "workspaces": [
        {
          "name": "Personal",
          "layout": "BSP"
        },
        {
          "name": "Work",
          "layout": "BSP",
          "initial_workspace_rules": [
            {
              "kind": "Exe",
              "id": "chrome.exe",
              "matching_strategy": "Equals"
            }
          ]
          //,
          //"window_container_behaviour_rules": {
          //  "0": "Create",
          //  "1": "Append"
          //}
        },
        {
          "name": "Other",
          "layout": "BSP"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "Code",
          "layout": "UltrawideVerticalStack"
        },
        {
          "name": "Chat",
          "layout": "BSP"
        },
        {
          "name": "Status",
          "layout": "Columns"
        }
      ]
    },
    {
      "workspaces": [
        {
          "name": "I",
          "layout": "UltrawideVerticalStack"
        },
        {
          "name": "II",
          "layout": "Columns"
        }
      ]
    }
  ],
  "transparency_ignore_rules": [
    {
      "kind": "Title",
      "id": "YouTube",
      "matching_strategy": "Contains"
    }
  ],
  "floating_applications": [
